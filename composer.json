{
    "name": "elixir/elixir-i18n",
    "description": "Component I18N",
    "version": "1.0.0",
    "minimum-stability": "dev",
    "license": "BSD-3-Clause",
    "keywords": [
        "elixir",
        "i18n"
    ],
    "homepage": "https://github.com/cedtanghe/elixir-i18n",
    "repositories": [
    ],
    "require": {
        "php": ">=5.6",
        "elixir/elixir-dispatcher" : "*",
        "elixir/elixir-config" : "*"
    },
    "require-dev": {
        "phpunit/PHPUnit": "~4.0",
        "sepia/po-parser": "*"
    },
    "autoload": {
        "psr-4": {
            "Elixir\\I18N\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elixir\\Test\\I18N\\": "tests/"
        }
    },
    "suggest": {
        "sepia/po-parser": "*",
        "ext-intl" : "*"
    }
}
